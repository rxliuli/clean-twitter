{
  "manifest_version": 3,
  "name": "Clean Twitter",
  "version": "0.5.1",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "src/assets/16.png",
    "32": "src/assets/32.png",
    "48": "src/assets/48.png",
    "128": "src/assets/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.ts-loader-d2cd06ac.js"
      ],
      "matches": [
        "https://twitter.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "resources": [
        "assets/_commonjsHelpers-912bb820.js",
        "assets/browser-polyfill-5a5eabde.js",
        "assets/loader-123353ca.js",
        "assets/config-e9f2c23c.js",
        "assets/injectHijackXHR.ts-77c8fda6.js",
        "assets/main.ts-3449db20.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
